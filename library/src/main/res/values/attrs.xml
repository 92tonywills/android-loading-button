<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LoadingButton">
        <attr name="buttonBackgroundTint" format="color" />
        <attr name="loadingColor" format="color" />
        <attr name="buttonTextDefault" format="string" />
        <attr name="buttonTextLoading" format="string" />
        <attr name="loadingPosition" format="enum">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>
</resources>